{
    "name": "angular-seed",
    "version": "1.0.0",
    "description": "Seed Project for Angular",
    "license": "MIT",
    "author": "Brittany Moore <brittanymoore>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/brittanymoore/angular-seed.git"
    },
    "scripts": {
        "build": "webpack --env=dev",
        "build:prod": "npm run clean && webpack --env=prod",
        "clean": "rimraf ./dist",
        "lint": "tslint -c ./config/tslint.json --type-check 'src/**/*.ts' -p ./tsconfig.json",
        "start": "webpack-dev-server --env=dev --open --hot",
        "start:prod": "npm run clean && webpack-dev-server --env=prod --open",
        "test": "karma start ./config/karma.conf.js ",
        "test:e2e": "protractor ./config/protractor.conf.js",
        "webdriver-manager": "webdriver-manager update --standalone false --gecko false"
    },
    "dependencies": {
        "@angular/animations": "4.3.6",
        "@angular/common": "4.3.6",
        "@angular/compiler": "4.3.6",
        "@angular/compiler-cli": "4.3.6",
        "@angular/core": "4.3.6",
        "@angular/forms": "4.3.6",
        "@angular/http": "4.3.6",
        "@angular/platform-browser": "4.3.6",
        "@angular/platform-browser-dynamic": "4.3.6",
        "@angular/platform-server": "4.3.6",
        "@angular/router": "4.3.6",
        "core-js": "2.5.1",
        "rxjs": "5.4.3",
        "zone.js": "0.8.17"
    },
    "devDependencies": {
        "@ngtools/webpack": "1.6.2",
        "@types/jasmine": "2.5.54",
        "@types/node": "8.0.27",
        "angular-router-loader": "0.6.0",
        "angular2-template-loader": "0.6.2",
        "autoprefixer": "7.1.4",
        "awesome-typescript-loader": "3.2.3",
        "codelyzer": "3.1.2",
        "css-loader": "0.28.7",
        "exports-loader": "0.6.4",
        "file-loader": "0.11.2",
        "html-webpack-plugin": "2.30.1",
        "jasmine": "2.8.0",
        "jasmine-core": "2.8.0",
        "karma": "1.7.1",
        "karma-chrome-launcher": "2.2.0",
        "karma-jasmine": "1.1.0",
        "karma-sourcemap-loader": "0.3.7",
        "karma-spec-reporter": "0.0.31",
        "karma-webpack": "2.0.4",
        "node-sass": "4.5.3",
        "postcss-loader": "2.0.6",
        "protractor": "5.1.2",
        "raw-loader": "0.5.1",
        "rimraf": "2.6.1",
        "sass-loader": "6.0.6",
        "ts-node": "3.3.0",
