{
    "name": "angular-vertex",
    "version": "0.0.1",
    "description": "Seed Project for Angular",
    "scripts": {
        "clean": "rimraf dev/*.js dev/*.map",
        "clean:prod": "rimraf dist/*.js",
        "build": "npm run clean && webpack --progress --config ./config/webpack.dev.js",
        "build:prod": "npm run clean:prod && webpack --progress --config ./config/webpack.prod.js",
        "server": "webpack-dev-server --config ./config/webpack.dev.js --inline --progress --port 3000 --content-base dev",
        "server:prod": "webpack-dev-server --config ./config/webpack.prod.js --inline --progress --port 3000 --content-base dist",
        "start": "npm run clean && npm run server",
        "start:prod": "npm run clean:prod && npm run server:prod",
        "test:unit": "karma start ./config/karma.conf.js ",
        "webdriver-manager": "webdriver-manager",
        "webdriver:update": "npm run webdriver-manager update",
        "protractor": "protractor ./config/protractor.conf.js",
        "test:e2e": "npm run protractor"
    },
    "license": "MIT",
    "dependencies": {
        "@angular/common": "2.3.1",
        "@angular/compiler": "2.3.1",
        "@angular/compiler-cli": "2.3.1",
        "@angular/core": "2.3.1",
        "@angular/forms": "2.3.1",
        "@angular/http": "2.3.1",
        "@angular/platform-browser": "2.3.1",
        "@angular/platform-browser-dynamic": "2.3.1",
        "@angular/platform-server": "2.3.1",
        "@angular/router": "3.3.1",
        "core-js": "2.4.1",
        "rxjs": "5.0.0-rc.4",
        "zone.js": "0.7.2",
        "ie-shim": "0.1.0"
    },
    "devDependencies": {
        "@types/node": "6.0.48",
        "@types/jasmine": "2.2.34",
        "@types/core-js": "0.9.35",
        "awesome-typescript-loader": "2.2.4",
        "angular2-router-loader": "0.3.4",
        "angular2-template-loader": "0.6.0",
        "typescript": "2.0.10",
        "css-loader": "0.25.0",
        "raw-loader": "0.5.1",
        "to-string-loader": "1.1.4",
        "webpack": "2.2.0",
        "webpack-dev-server": "2.2.0",
        "webpack-merge": "0.15.0",
        "html-webpack-plugin": "2.24.1",
        "rimraf": "2.5.4",
        "karma": "1.2.0",
        "karma-jasmine": "1.0.2",
        "karma-chrome-launcher": "2.0.0",
        "karma-webpack": "2.0.2",
        "karma-sourcemap-loader": "0.3.7",
        "karma-spec-reporter": "0.0.26",
        "jasmine": "2.5.2",
        "protractor": "4.0.10",
        "ts-node": "1.7.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/brittanymoore/angular-vertex.git"
    }
}